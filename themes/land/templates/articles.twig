{% extends "base.twig" %}

{% block page_title %}Статьи{% endblock %}

{% block page_header %}
    <div class="page-header">
        <h1>Статьи</h1>
    </div>
{% endblock %}

{% block body_class %}blog{% endblock %}

{% block page_contents %}
    <ul>
        {% for e in entries %}
            <li>
                <a href='{{ e.link }}'>{{ e.title }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
